        <arguments>
          <argument>
            <kind>1</kind>
            <string>///deck editor
var len=9
var _ylevel=0
var _xlevel=0
if IsTempDeckEditor{
    for(var i=1;i&lt;(UnitID.UnitID_MAX);i++){
        var _sc=0.2
        var _sp=global.UnitDat_Sprite[i]
        var _im=global.UnitDat_Image[i]
        var _op=1
        if ds_list_size(global.PlayerDeck)&gt;20{
            _op=0.2
        }
        if ds_list_find_index(global.PlayerDeck,i)!=-1{
            _op=0.2
        }
        
        var _x=100+200*_xlevel
        var _y=100+250*_ylevel+scrollbarY*100
        _xlevel++
        draw_sprite_ext(_sp,_im,_x,_y,0.2,0.2,0,c_white,_op)
        if _xlevel=len{
            _ylevel++
            _xlevel=0
        }
        var msx=device_mouse_x_to_gui(0)
        var msy=device_mouse_y_to_gui(0)
        
        if mouse_check_button_pressed(mb_left){
        if point_in_rectangle(msx,msy,
            _x-sprite_get_width(_sp)/2*_sc,
            _y-sprite_get_height(_sp)/2*_sc,
            _x+sprite_get_width(_sp)/2*_sc,
            _y+sprite_get_height(_sp)/2*_sc,
        ){
            if _op=1{
                ds_list_add(global.PlayerDeck,i)
            }else{
                ds_list_delete(global.PlayerDeck,ds_list_find_index(global.PlayerDeck,i))
            }
        }
        }
    }
    if mouse_wheel_up(){
        scrollbarY++
    }
    if mouse_wheel_down(){
        scrollbarY--
    }
    
    if keyboard_check_pressed("1"){
        clipboard_set_text(ds_list_write(global.PlayerDeck))
    }
    if keyboard_check_pressed("2"){
        ds_list_read(global.PlayerDeck,clipboard_get_text())
    }
}
</string>
